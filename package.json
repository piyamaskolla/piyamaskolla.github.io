{
  "name": "piyamaskolla.github.io",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "init": "docsify init .",
    "serve": "docsify serve --livereload-port 8081 --port 8080 .",
    "lint": "eslint js/shelo.js",
    "dir-rm-css": "rimraf css",
    "dir-mk-css": "mkdir css",
    "css": "stylus src/css/website.styl -u autoprefixer-stylus -o css",
    "cssc": "stylus src/css/website.styl -c -u autoprefixer-stylus -o css",
    "css-min": "node build/css-min.js",
    "watch-css": "run-p 'css -- -w'",
    "build-css": "run-s dir-rm-css dir-mk-css css css-min",
    "dir-rm-js": "rimraf js",
    "dir-mk-js": "mkdir js",
    "dir-cp-js": "cp -R src/js-static/* js",
    "js-min": "node build/js-min.js",
    "build-js-dev": "NODE_ENV=development run-s dir-rm-js dir-mk-js dir-cp-js js-min",
    "build-js": "NODE_ENV=production run-s dir-rm-js dir-mk-js dir-cp-js js-min",
    "lib-init": "lib init",
    "lib-create": "lib create tracking",
    "lib-http": "cd piyamaskolla/tracking && lib http",
    "lib-dev": "cd piyamaskolla/tracking && lib up dev",
    "lib-release": "cd piyamaskolla/tracking && lib release",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test1": "docsify serve --help"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piyamaskolla/piyamaskolla.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/piyamaskolla/piyamaskolla.github.io/issues"
  },
  "homepage": "https://github.com/piyamaskolla/piyamaskolla.github.io#readme",
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "cssnano": "^4.1.7",
    "docsify-cli": "^4.3.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "lib": "^3.0.2",
    "lib.cli": "^4.4.2",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.3",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "stylus": "^0.54.5"
  }
}
